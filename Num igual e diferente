#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int n1, n2;

    printf("Digite dois numeros inteiros: ");
    scanf("%d%d", &n1, &n2);

    if (n1 == n2){
        printf("Os numeros digitados sao iguais! :D ");
    }
    else{
        printf("Os numeros digitados sao diferentes! :) ");
    }

  return 0;
}
